{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "OPTIONS, POST",
                "Access-Control-Allow-Headers": "Content-Type, Origin"
            },
            "continue": true
        },
        {
            "src": "/api/generate",
            "methods": ["OPTIONS", "POST"],
            "dest": "/api/generate.js"
        }
    ]
}
