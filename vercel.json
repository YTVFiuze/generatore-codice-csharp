{
    "version": 2,
    "builds": [
        {
            "src": "api/generate.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/generate",
            "dest": "/api/generate.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}
