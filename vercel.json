{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "public",
    "builds": [
        { "src": "api/generate.js", "use": "@vercel/node" },
        { "src": "public/**/*", "use": "@vercel/static" }
    ],
    "routes": [
        { "src": "/api/generate", "dest": "/api/generate.js" },
        { "src": "/(.*)", "dest": "/public/$1" }
    ],
    "functions": {
        "api/generate.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}
